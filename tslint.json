{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-prettier"
  ],
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "singleline": "never"
      }
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "max-line-length": [
      true,
      80
    ],
    "no-any": false
  },
  "jsRules": {},
  "rulesDirectory": []
}